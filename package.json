{
  "name": "quick-draw",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "start-server-old": "nodemon --ext ts --watch server --watch .env src/server/index.ts",
    "start-server": "node scripts/start-server.js",
    "start-client": "node scripts/start-client.js",
    "prod": "npm run build && PORT=3000 NODE_ENV=production npm run start-server",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mongodb": "^3.0.10",
    "nodemon": "^1.17.5",
    "object-assign": "^4.1.1",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/enzyme": "^3.1.11",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.5",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/mongodb": "^3.0.21",
    "@types/node": "^10.3.1",
    "@types/passport": "^0.4.5",
    "@types/passport-github": "^1.1.3",
    "@types/passport-local": "^1.0.33",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.0",
    "@types/sinon": "^5.0.1",
    "@types/webpack": "^4.4.5",
    "autoprefixer": "^8.6.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "canvas": "^1.6.11",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.0",
    "jest-canvas-mock": "^1.1.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "resolve": "^1.8.1",
    "sinon": "^6.1.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "tsconfig-paths": "^3.4.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-serve": "^2.0.2",
    "whatwg-fetch": "^2.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules"
    ],
    "execMap": {
      "ts": "ts-node -P tsconfig.server.json"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/node_modules/tsconfig-paths/register.js",
      "<rootDir>/config/jest/testSetup.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/[^flycheck]*.(j|t)s?(x)",
      "<rootDir>/src/**/[^flycheck]?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "shared/.*": "<rootDir>/src/$0"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "mjs",
      "node",
      "ts",
      "tsx",
      "web.js",
      "web.jsx",
      "web.ts",
      "web.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
